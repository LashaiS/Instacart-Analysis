   "source": [
    "def family_status(n):\n",
    "    if n == 0:\n",
    "        return \"No Dependents\"\n",
    "    else:\n",
    "        return \" Has Dependants\"\n",
    "\n",
    "df[\"family_status\"] = df[\"n_dependants\"].apply(family_status)"
   ]
  },
  {
   "cell_type": "code",
