   "cell_type": "code",
   "execution_count": 33,
   "id": "129f119c-a1bb-4b59-999a-6c4faf1feacf",
   "metadata": {},
   "outputs": [],
   "source": [
    "def customer_profile(row):\n",
    "    if row[\"age_group\"] == \"Young Adult\" and row[\"spending_flag\"] == \"Low Spender\":\n",
    "        return \"Young Budget Shopper\"\n",
    "    elif row[\"family_status\"] !=\"No Dependents\" and row[\"income_group\"] == \"High Income\":\n",
    "        return \"High_Income Family Shopper\"\n",
    "    elif row[\"age_group\"] == \"Senior\":\n",
    "        return \"Senior Shopper\"\n",
    "    elif row[\"frequency_flag\"] == \"Regular customer\":\n",
    "        return \"Routine Shopper\"\n",
    "    else:\n",
    "        return \"General Shopper\"\n",
    "\n",
    "df[\"customer_profile\"] = df.apply(customer_profile, axis=1)"
   ]
  },
